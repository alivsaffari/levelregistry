{
    "version": 2,
    "builds": [
        {
            "src": "levelCampBackend/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "./staticfiles"
            }
        }
    ],
    "routes": [
        {
            "src": "/statics/(.*)",
            "dest": "/statics/$1"
        },
        {
            "src": "/(.*)",
            "dest": "levelCampBackend/wsgi.py"
        }
    ],
    "outputDirectory": "./staticfiles"
}